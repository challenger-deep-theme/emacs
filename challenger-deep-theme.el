      (ct-magenta     "#d7afff")
      (magenta-d      "#906cff")
      (ct-magenta-d   "#875fd7")
      (cyan           "#aaffe4")
      (ct-cyan        "#87ffff")
      (cyan-d         "#63f2f1")
      (ct-cyan-d      "#00ffff")
      (blue           "#91ddff")
