(deftheme challenger-deep
  "challenger-deep theme")

(let ((ct '((class color) (min-colors 89)))
      (c '((class color) (min-colors 257)))
      (bold           t)
      (italic         t)

      (black          "#100e23")
      (ct-black       "#121212")
      (white          "#cbe3e7")
      (ct-white       "#eeeeee")
      (grey           "#cbe3e7")
      (ct-grey        "#eeeeee")
      (grey-d         "#a6b3cc")
      (ct-grey-d      "#b2b2b2")
      (grey-dd        "#565575")
      (ct-grey-dd     "#808080")
      (grey-ddd       "#2b2942")
      (ct-grey-ddd    "#3a3a3a")
      (yellow         "#ffe9aa")
      (ct-yellow      "#ffd75f")
      (yellow-d       "#ffb378")
      (ct-yellow-d    "#ffaf5f")
      (red-d          "#ff5458")
      (ct-red-d       "#ff5f00")
      (red-dd         "#8e3939")
      (ct-red-dd      "#800000")
      (red            "#ff8080")
      (ct-red         "#ff8787")
